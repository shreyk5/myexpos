int main()
{
decl
	str prompt;
	int tmp;
enddecl

begin	
	prompt = "Enter file name";
	tmp = exposcall("Write",-2,prompt);
	tmp = exposcall("Read",-1,prompt);
	tmp = exposcall("Exec",prompt);
	
	if(tmp == -1) then
		prompt = "Exec call unsuccessful";
		tmp = exposcall("Write",-2,prompt);
	endif;
	
	return 0;
end
}
