[PROCESS_TABLE+([SYSTEM_STATUS_TABLE+1]*16)+4]=TERMINATED;

alias curr R0;
alias not_running R1;
not_running = 1;
curr = 1;

while(curr < 16) do
	if([PROCESS_TABLE+(curr*16)+4] != TERMINATED) then
		not_running = 0;
		break;
	endif;
	curr = curr + 1;
endwhile;

if(not_running == 1) then
	halt;
else
	call MOD_5;
endif;
